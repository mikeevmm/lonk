{
  "version": 1,
  "db": {
    "address": "redis://redis:6379",
    "expire_seconds": 259200
  },
  "slug_rules": {
    "length": 5,
    "chars": "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789_-"
  },
  "log_rules": {
    "error_log_file": "/data/log/error.log",
    "access_log_file": "/data/log/access.log"
  },
  "serve_rules": {
    "dir": {
      "Dir": "/data/served"
    },
    "addr": {
      "ip": "0.0.0.0",
      "port": 8080
    }
  }
}
